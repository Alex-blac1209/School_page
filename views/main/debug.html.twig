{% extends 'base.html.twig' %}

{% block body %}
    <div class='container'>
        <h3>This is Debug Page!</h3>
        <p>There was some error in page</p>
        <p>{{ debug }}</p>

        {% for user in users %}
            <ul>
                <li>Id: {{ user.id }}</li>
                <li>Name: {{ user.name }}</li>
                <li>Email: {{ user.email }}</li>
            </ul>
        {% endfor %}
    </div>
{% endblock body %}